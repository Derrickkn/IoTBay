<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>IoTDevicesServlet</servlet-name>
        <servlet-class>uts.isd.controller.IoTDevicesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>uts.isd.controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>uts.isd.controller.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>uts.isd.controller.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditServlet</servlet-name>
        <servlet-class>uts.isd.controller.EditServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeactivateAccServlet</servlet-name>
        <servlet-class>uts.isd.controller.DeactivateAccServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditDeviceServlet</servlet-name>
        <servlet-class>uts.isd.controller.EditDeviceServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminDashboardServlet</servlet-name>
        <servlet-class>uts.isd.controller.adminDashboardServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditServlet</servlet-name>
        <url-pattern>/EditServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeactivateAccServlet</servlet-name>
        <url-pattern>/DeactivateAccServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IoTDevicesServlet</servlet-name>
        <url-pattern>/IoTDevices</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditDeviceServlet</servlet-name>
        <url-pattern>/EditDevice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminDashboardServlet</servlet-name>
        <url-pattern>/adminDashboardServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
